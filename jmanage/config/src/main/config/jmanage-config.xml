<jmanage-config>

    <authentication class="org.jmanage.common.auth.FileAuthentication"/>

    <application-modules>
        <module
            id="weblogic"
            name="Weblogic 6.1"
            configClass="org.jmanage.core.modules.weblogic.WeblogicApplicationConfig"
            connectionFactory="org.jmanage.core.modules.weblogic.WLServerConnectionFactory"/>
        <module
            id="tomcat"
            name="Tomcat 4.x &amp; 5.x"
            configClass="org.jmanage.core.modules.tomcat.TomcatApplicationConfig"
            connectionFactory="org.jmanage.core.modules.tomcat.TomcatServerConnectionFactory"/>
        <module
            id="jsr160"
            name="JSR160"
            configClass="org.jmanage.core.modules.jsr160.JSR160ApplicationConfig"
            connectionFactory="org.jmanage.core.modules.jsr160.JSR160ServerConnectionFactory"/>
        <module
            id="jboss"
            name="JBoss 3.2.4"
            configClass="org.jmanage.core.modules.jboss.JBossApplicationConfig"
            connectionFactory="org.jmanage.core.modules.jboss.JBossServerConnectionFactory"/>
        <module
            id="websphere"
            name="WebSphere 5x &amp; 6"
            configClass="org.jmanage.core.modules.websphere.WebSphereApplicationConfig"
            connectionFactory="org.jmanage.core.modules.websphere.WebSphereServerConnectionFactory"/>
        <module
            id="snmp"
            name="SNMP"
            configClass="org.jmanage.core.modules.snmp.SNMPApplicationConfig"
            connectionFactory="org.jmanage.core.modules.snmp.SNMPAgentConnectionFactory"/>
    </application-modules>

</jmanage-config>